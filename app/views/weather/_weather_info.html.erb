<img src="tempicons/clouds.svg" alt="" id="tempIcon">
<div class="flex">
  <h2 id="weatherCountry"><%= weather[:country] %></h2>
  <h2 id="temperature"><%= weather[:temperature] %>°<strong>C</strong></h2>
  <h2 id="weatherDescription"><%= weather[:description] %></h2>
  <ul>
    <div class="features">
      <li id="feelsLike">Feels Like <%= weather[:feels_like] %>°C</li>
      <li id="humidity">Humidity <%= weather[:humidity] %></li>
      <li id="longitude">Longitude <%= weather[:longitude] %></li>
      <li id="latitude">Latitude <%= weather[:latitude] %></li>
    </div>
  </ul>
  <div class="h1">Done by Mina</div>
</div>
